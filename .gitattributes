# Git LFS configuration for images
public/vehicles/*.png filter=lfs diff=lfs merge=lfs -text
public/images/**/*.png filter=lfs diff=lfs merge=lfs -text
public/images/**/*.jpg filter=lfs diff=lfs merge=lfs -text
public/images/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
public/data/*.json filter=lfs diff=lfs merge=lfs -text
data/processed/*.json filter=lfs diff=lfs merge=lfs -text
